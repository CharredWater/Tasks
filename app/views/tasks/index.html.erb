<h1>Tasks#index</h1>
<p>Find me in app/views/tasks/index.html.erb</p>
<b>Hello World</b>

<%= render 'form' %>


<u1>
	<% for task in @tasks %>
		<li id="task_<%= task.id%>">
			<%= task.task%>
			| <%= link_to 'Edit', edit_task_path(task) %>
			| <%= link_to 'Delete', task, :method => 'delete' %>
		</li>
	<%end%>
</u1>
